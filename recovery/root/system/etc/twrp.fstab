# mount point       fstype    device                                                                flags
/system                  ext4     /dev/block/platform/bootdevice/by-name/system       flags=display="System";backup=1;slotselect;flashimg=0;wipeingui
/system_image            emmc     /dev/block/platform/bootdevice/by-name/system       flags=slotselect;flashimg=1
/vendor                  ext4     /dev/block/platform/bootdevice/by-name/vendor       flags=display="Vendor";backup=1;slotselect;flashimg=0;wipeingui
/vendor_image            emmc     /dev/block/platform/bootdevice/by-name/vendor       flags=slotselect;flashimg=1
/data                    ext4     /dev/block/platform/bootdevice/by-name/userdata     flags=display="Data";backup=1;wipeingui;encryptable=/dev/block/platform/bootdevice/by-name/metadata
/protect_f               ext4     /dev/block/platform/bootdevice/by-name/protect1     flags=display="Protect_f";backup=1
/protect_s               ext4     /dev/block/platform/bootdevice/by-name/protect2     flags=display="Protect_s";backup=1
/nvdata                  ext4     /dev/block/platform/bootdevice/by-name/nvdata       flags=display="Nvdata";backup=1
/persist                 ext4     /dev/block/platform/bootdevice/by-name/persist      flags=display="Persist";backup=1
/persistent              emmc     /dev/block/platform/bootdevice/by-name/frp          flags=display="Persistent";backup=1
/nvram                   emmc     /dev/block/platform/bootdevice/by-name/nvram        flags=display="Nvram";backup=1
/proinfo                 emmc     /dev/block/platform/bootdevice/by-name/proinfo      flags=display="Proinfo";backup=1
/misc                    emmc     /dev/block/platform/bootdevice/by-name/para         flags=display="Misc";backup=1
/logo                    emmc     /dev/block/platform/bootdevice/by-name/logo         flags=display="Logo";backup=1
/expdb                   emmc     /dev/block/platform/bootdevice/by-name/expdb        flags=display="Expdb";backup=1
/secro                   emmc     /dev/block/platform/bootdevice/by-name/secro        flags=display="Secro";backup=1
/seccfg                  emmc     /dev/block/platform/bootdevice/by-name/seccfg       flags=display="Seccfg";backup=1
/bootloader              emmc     /dev/block/platform/bootdevice/by-name/lk           flags=display="Bootloader";slotselect;backup=1;flashimg=1
/boot                    emmc     /dev/block/platform/bootdevice/by-name/boot         flags=display="Boot";backup=1;flashimg=1;slotselect
/md1img                  emmc     /dev/block/platform/bootdevice/by-name/md1img       flags=display="Md1img";slotselect;backup=1
/md1dsp                  emmc     /dev/block/platform/bootdevice/by-name/md1dsp       flags=display="Md1dsp";slotselect;backup=1
/md1arm7                 emmc     /dev/block/platform/bootdevice/by-name/md1arm7      flags=display="Md1arm7";slotselect;backup=1
/md3img                  emmc     /dev/block/platform/bootdevice/by-name/md3img       flags=display="Md3img";slotselect;backup=1
/ppl                     emmc     /dev/block/platform/bootdevice/by-name/ppl          flags=display="Ppl";backup=1
/tee                     emmc     /dev/block/platform/bootdevice/by-name/tee          flags=display="Tee";slotselect;backup=1
/external_sd             vfat     /dev/block/mmcblk1p1                                flags=display="SD-Card";backup=0;wipeingui;storage;removable
/usb-otg                 auto     /dev/block/sda1                                     flags=display="USB OTG";backup=0;wipeingui;storage;removable
